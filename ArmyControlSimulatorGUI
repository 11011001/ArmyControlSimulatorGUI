--Tabs
BiomeTP.MouseButton1Down:connect(function()
	BiomeTPFrame.Visible = true
	OtherusefulFrame.Visible = false
	FarmingFrame.Visible = false
	ShopTPFrame.Visible = false
end)

Otheruseful.MouseButton1Click:connect(function()
	BiomeTPFrame.Visible = false
	OtherusefulFrame.Visible = true
	FarmingFrame.Visible = false
	ShopTPFrame.Visible = false
end)

Farming.MouseButton1Click:connect(function()
	BiomeTPFrame.Visible = false
	OtherusefulFrame.Visible = false
	FarmingFrame.Visible = true
	ShopTPFrame.Visible = false
end)

ShopTP.MouseButton1Click:connect(function()
	BiomeTPFrame.Visible = false
	OtherusefulFrame.Visible = false
	FarmingFrame.Visible = false
	ShopTPFrame.Visible = true
end)
