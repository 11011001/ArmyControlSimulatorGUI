end)

Spawnleaderboards.MouseButton1Click:connect(function()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1318.60229, 87.5853806, 35.5183754)
end)

Tundra.MouseButton1Click:connect(function()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-86.2176208, 258.646515, 16.429533)
end)

--Auto Farms
BeginnerON.MouseButton1Click:connect(function()
	game.Players.LocalPlayer.character.HumanoidRootPart.CFrame = CFrame.new(1097.82, 88.544, 42.22)

wait(4)

_G.beginner = true

while _G.beginner == true do
wait(0.4)
for _,v in pairs(workspace.Entities.farmerModel:GetDescendants()) do
if v.Name == "Torso" then
game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
end
end
end
end)

BeginnerOFF.MouseButton1Click:connect(function()
	_G.beginner = false

while _G.beginner == true do
wait(0.4)
for _,v in pairs(workspace.Entities.farmerModel:GetDescendants()) do
if v.Name == "Torso" then
game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
end
end
end
end)

