-- Scripts:
--Other Usefull
ClaimBase.MouseButton1Click:connect(function()
	local a = "addUser"
local b = game:GetService("Workspace").Spawns["DEFAULT_SPAWN"]
game:GetService("ReplicatedStorage").Event:FireServer(a, b)
end)

AutoWandON.MouseButton1Click:connect(function()
	_G.Wand = true

while _G.Wand == true do
wait(0.1)
local char = game.Players.LocalPlayer.character.HumanoidRootPart.CFrame.Position
local a = "use"
local b = "wand"
local c = "place"
local d = char
game:GetService("ReplicatedStorage").Event:FireServer(a, b, c, d)
end
end)

AutoWandOFF.MouseButton1Click:connect(function()
	_G.Wand = false

while _G.Wand == true do
wait(0.1)
local char = game.Players.LocalPlayer.character.HumanoidRootPart.CFrame.Position
local a = "use"
local b = "wand"
local c = "place"
local d = char
game:GetService("ReplicatedStorage").Event:FireServer(a, b, c, d)
end
end)

